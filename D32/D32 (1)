public class VariabelJava {
    public static void main(String[] args) {
        
        int angka = 5;
        double pecahan = 3.14;
        String teks = "Ini adalah contoh variabel";

        System.out.println("Nilai angka: " + angka);
        System.out.println("Nilai pecahan: " + pecahan);
        System.out.println("Nilai teks: " + teks);

     
        angka = 10;
        pecahan = 2.718;
        teks = "Variabel telah diubah";

        System.out.println("Nilai angka setelah diubah: " + angka);
        System.out.println("Nilai pecahan setelah diubah: " + pecahan);
        System.out.println("Nilai teks setelah diubah: " + teks);
  
      }
}